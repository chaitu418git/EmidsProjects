package practice17;

public class MissingNumber {

	public int sumOfElements(int n)
	{
		return (n*(n+1)/2);
	}
	public int sumArray(int[] array)
	{
		int sum=0;
		for(int i=0;i<array.length;i++)
		{
			sum=sum+array[i];
		}
		return sum;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
