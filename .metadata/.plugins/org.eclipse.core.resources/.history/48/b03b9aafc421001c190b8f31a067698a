package practice17;

public class SepZeroesToFirst {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int []array= {1,0,0,2,4,0,5,0,6};

		int counter=0;
		for(int i=0;i<array.length;i++)
		{
			if(array[i]!=0)
			{
				array[counter]=array[i];
				counter++;
			}
			
		}
		
	}

}
