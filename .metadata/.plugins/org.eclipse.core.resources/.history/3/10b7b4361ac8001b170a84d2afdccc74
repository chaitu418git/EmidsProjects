package javaprograms;

public class PyramidTriangleex1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
/*4321
 *765
 *98
 *10
 * 
 */
		int k=0;
		for(int i=0;i<4;i++)
		{
			for(int j=4-k;j>0;j--)
			{
				
				System.out.print(j);
				System.out.print("\t");
				k=j;
				k++;
			}
			System.out.println("");
		}
	}

}
