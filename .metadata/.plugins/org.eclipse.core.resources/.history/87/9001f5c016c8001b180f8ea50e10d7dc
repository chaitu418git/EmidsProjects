package javaprograms;

public class PyramidTriangle {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
/*1234
 *456
 *78
 *9 
 */
		for(int i=0;i<4;i++)
		{
			for(int j=1;j<=4-i;j++)
			{
				System.out.print(j);
				//System.out.println();
			}
			System.out.print("");
		}
	}

}
