package javaprograms;

public class PyramidTriangleex1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
/*4321
 *765
 *98
 *10
 * 
 */
		//int k=1;
		for(int i=0;i<4;i++)
		{
			int k=4-i;
			for(int j=k;j>0;j--)
			{
				
				System.out.print(j);
				System.out.print("\t");
				
				
			}
			
			//k++;
			System.out.println("");
		}
	}

}
